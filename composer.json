{
    "name": "russofinn/laravel-interactions",
    "description": "A very simple package to record user interactions on your website or application.",
    "keywords":
    [
        "interactions",
        "likes",
        "views",
        "comments",
        "laravel"
    ],
    "authors": [
        {
            "name": "Weslley Almeida",
            "email": "almeida.weslley577@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Russofinn\\Interactions\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Russofinn\\Interactions\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Russofinn\\Interactions\\InteractionsServiceProvider"
            ]
        }
    }
}
